<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="SeminarTitle" parent="@android:style/TextAppearance.Medium">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center_horizontal</item>
        <item name="android:paddingTop">10dp</item>
        <item name="android:textColor">#FFFFFF</item>
        <item name="android:typeface">monospace</item>
    </style>
    <style name="SeminarPanelist" parent="@android:style/TextAppearance.Small">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center_horizontal</item>
        <item name="android:paddingTop">5dp</item>
        <item name="android:paddingBottom">10dp</item>
        <item name="android:textColor">#FFFFFF</item>
        <item name="android:typeface">monospace</item>
    </style>
    <style name="SeminarInfo">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center_horizontal</item>
        <item name="android:textSize">10dp</item>
        <item name="android:textColor">#FFFFFF</item>
        <item name="android:layout_weight">0.5</item> 		        
        <item name="android:typeface">monospace</item>
    </style>
    <style name="SeminarAbstract" parent="@android:style/TextAppearance.Small">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:textColor">#FFFFFF</item>
        <item name="android:typeface">monospace</item>
    </style>
</resources>